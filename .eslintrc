{
  "extends": ["airbnb-base"],
  "env": {
    "jest": true
  },
  "root": true,
  "rules": {
    "arrow-parens": ["warn", "as-needed"],
    "no-param-reassign": 0,
    "consistent-return": 0,
    "global-require": 0,
    "max-len": 0,
    "no-unused-expressions": 0,
    "no-tabs": 0,
    "valid-typeof": 0,
    "no-underscore-dangle": 0,
    "no-throw-literal": 0,
    "quotes": ["error", "double"]
  },
  "overrides": [
    {
      "files": ["*.test.js"],
      "rules": {
        "no-undef": 0,
        "no-console": 0
      }
    }
  ]
}
